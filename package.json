{
  "name": "keplers-laws",
  "version": "1.3.0-dev.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/phetsims/keplers-laws.git"
  },
  "devDependencies": {
    "grunt": "~1.5.3"
  },
  "phet": {
    "requirejsNamespace": "KEPLERS_LAWS",
    "simulation": true,
    "runnable": true,
    "supportedBrands": [
      "phet",
      "adapted-from-phet",
      "phet-io"
    ],
    "phetLibs": [
      "bamboo",
      "my-solar-system",
      "solar-system-common"
    ],
    "phet-io": {
      "compareDesignedAPIChanges": true
    },
    "supportsOutputJS": true,
    "simFeatures": {
      "supportsDynamicLocale": true,
      "supportsSound": true,
      "colorProfiles": [
        "default",
        "projector"
      ],
      "supportsInteractiveDescription": true
    },
    "screenNameKeys": [
      "KEPLERS_LAWS/screen.firstLaw",
      "KEPLERS_LAWS/screen.secondLaw",
      "KEPLERS_LAWS/screen.thirdLaw",
      "KEPLERS_LAWS/screen.allLaws"
    ],
    "published": true
  }
}